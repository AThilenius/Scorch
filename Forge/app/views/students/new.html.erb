<form id="createForm" role="form-signin" action="create">
  <div class="thinWindow">
    <h3 class="text-center"> Student Info </h3>
    <hr>

    <div class="form-group">
      <label for="username">CU Login</label>
      <input class="form-control" type="username" name="username" placeholder="CU Login">
    </div>

    <div class="form-group">
      <label for="firstName">First Name</label>
      <input class="form-control" type="firstName" name="firstName" placeholder="First Name">
    </div>

    <div class="form-group">
      <label for="lastName">Last Name</label>
      <input class="form-control" type="lastName" name="lastName" placeholder="Last Name">
    </div>

    <div class="form-group">
      <label for="studentID">Student ID</label>
      <input class="form-control" type="studentID" name="studentID" placeholder="Student ID">
    </div>

    <div class="form-group">
      <label for="arenaLocation">Arena Location</label>
      <input class="form-control" type="arenaLocation" name="arenaLocation" placeholder="Ex: 0, 0, 0">
    </div>

    <button type="submit" class="btn btn-default btn-primary autoWidth">Create</button>

    <hr>
  </div>
</form>
<div class="thinWindow">
  <p class="small text-center">A password will be automatically generated.</p>
</div>